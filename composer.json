{
    "name": "everestmx/belongs-to-many-field",
    "description": "BelongsToMany nova representation in field.",
    "keywords": [
        "laravel",
        "nova",
        "belongsToMany",
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Everestmx\\BelongsToManyField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Everestmx\\BelongsToManyField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
